
.swiper{
  margin-bottom: 24rpx;
}

.swiper image{
  width: 100%;
  height: 100%
}

.menus{
  display: flex;
  padding: 20rpx 0;
  justify-content: space-around;
}

.menus image{
  width: 128rpx;
  height: 140rpx;
}

.floor-title image{
  width: 100%;
  height: 60rpx;
}

.floor{
  display: flex;
  justify-content: space-between;
  padding: 16rpx;
}

.floor .left image{
  width: 232rpx;
  height: 390rpx;
}

.floor .right{
  display: flex;
  flex: 1;
  justify-content: space-between;
  flex-wrap: wrap;
  padding-left: 20rpx;
}

.floor .right image{
  width:230rpx;
  height: 190rpx;
}
